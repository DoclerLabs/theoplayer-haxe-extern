package theoplayer.controller;

/**
 * Class representing a playlist item.
 */
extern class PlaylistItem {

  /**
   * Returns the title of the playlist item.
   */
  public var title: String;

  /**
   * Returns the description of the playlist item.
   */
  public var description: String;

  /**
   * Returns the poster for the playlist item.
   */
  public var poster: Dynamic;

  /**
   * Returns the source of the playlist item.
   */
  public var src: String;

  /**
   * Returns the startFrame of the playlist item.
   */
  public var startFrame: Dynamic;

  /**
   * Returns the endFrame of the playlist item.
   */
  public var endFrame: Dynamic;

  /**
   * Returns the startTime of the playlist item.
   */
  public var startTime: Dynamic;

  /**
   * Returns the endTime of the playlist item.
   */
  public var endTime: Dynamic;
}
